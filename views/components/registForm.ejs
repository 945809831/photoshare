<%# 注册窗口%>
<form role="form">
    <div class="form-group">
        <label for="email">电子邮件地址</label>
        <input class="form-control" id="email" type="email" name="email" placeholder="电子邮件地址">
    </div>
    <div class="form-group">
        <label for="pwd">密码</label>
        <input class="form-control" id="pwd" type="password" name="pwd" placeholder="密码">
    </div>
    <div class="form-group">
        <label for="pwd2">确认密码</label>
        <input class="form-control" id="pwd2" type="password" name="pwd" placeholder="确认密码">
    </div>
    <button class="btn btn-default" type="submit" name="button">确定注册</button>
</form>
</div>
